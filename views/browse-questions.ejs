<!DOCTYPE html>
<html lang="en">
  <%- include("partials/head.ejs")%>
  <body>
    <%- include("partials/header.ejs") %>

    <div class="container">
      <h1 class="mb-4 border-bottom">Browse Questions</h1>
      <% if(questions.length > 0){ %> <% questions.forEach(item =>{ %>
      <ul class="list-group list-group-flush">
        <li class="list-group-item list-group-item-action browse-item">
          <p class="browse-question"><strong><%= item.question %></strong></p>
          <p class="browse-answer"><em><%= item.answer %></em></p>
        </li>
      </ul>
      <% }); %> <% } %>
    </div>

    <%- include("partials/footer.ejs") %>
  </body>
</html>
